{
  "expo": {
    "name": "superlift",
    "slug": "superlift",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "superlift",
    "plugins": [
      ["expo-font", {
        "fonts": [
          "./assets/fonts/archivoblack.ttf",
          "./assets/fonts/inter.ttf"
        ]
      }],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
